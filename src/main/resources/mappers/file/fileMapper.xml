<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.care.bedu.file.dao.FileUploadDao">

    <select id="getLectureList" resultType="com.care.bedu.file.vo.FileUploadVO">
        SELECT
            LECT_NUM, 
            TITLE,
            TEACHER,
            LECT_SUMMARY,
            CATE_CODE,
            CATE_KOR,
            PRICE,
            LECT_PERIOD
        FROM
            BEDU.T_BEDU_LECT
        WHERE TITLE LIKE '%'||#{keyword}||'%'
    </select>


</mapper>